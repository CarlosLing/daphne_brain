{
  "params":
  [
    { "name": "measurement", "type": "measurement", "mandatory": true },
    { "name": "year1", "type": "year", "mandatory": false },
    { "name": "year2", "type": "year", "mandatory": false }
  ],
  "query":
  {
    "always": "session.query(models.Mission).join(models.Instrument).filter(models.Instrument.measurements.any(models.Measurement.ilike('%${measurement}%'))",
    "opt":
    [
      { "cond": "year1", "query_part": ".filter(models.Mission.launch_date > ${year1})" },
      { "cond": "year2", "query_part": ".filter(models.Mission.launch_date < ${year2})" }
    ],
    "end": ".order_by(Mission.launch_date)",
    "result_type": "list",
    "result_field": "name"
  },
  "response": "The missions that measure ${measurement} are ${response}"
}