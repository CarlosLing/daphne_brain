{
  "params":
  [
    { "name": "measurement", "type": "measurement", "options": "", "mandatory": true },
    { "name": "year1", "type": "year", "options": "begin", "mandatory": false },
    { "name": "year2", "type": "year", "options": "end", "mandatory": false }
  ],
  "query":
  {
    "always": "session.query(models.Mission).join(models.Instrument, models.Mission.instruments).filter(models.Instrument.measurements.any(models.Measurement.name.ilike('%${measurement}%')))",
    "opt":
    [
      { "cond": "year1", "query_part": ".filter(models.Mission.launch_date > data['year1'])" },
      { "cond": "year2", "query_part": ".filter(models.Mission.launch_date < data['year2'])" }
    ],
    "end": ".order_by(models.Mission.launch_date)",
    "result_type": "list",
    "result_field": "name"
  },
  "response": "The missions that measure ${measurement} are ${response}"
}